{
    "manifest_version": 3,
    "name": "Hide Twitter Blue",
    "version": "1",
    "description": "This extension hides the Twitter Blue button on Twitter.",
    "host_permissions": [
      "activeTab"
    ],
    "action": {
      "default_title": "Hide Twitter Blue",
      "default_popup": "menu.html"
    },
    "content_scripts": [
      {
        "matches": [
          "https://twitter.com/*"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }